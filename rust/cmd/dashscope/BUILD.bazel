load("@rules_rust//rust:defs.bzl", "rust_binary")
load("@crate_index//:defs.bzl", "aliases", "all_crate_deps")

package(default_visibility = ["//visibility:public"])

rust_binary(
    name = "dashscope",
    srcs = glob(["src/**/*.rs"]),
    aliases = aliases(),
    deps = [
        "//rust/dashscope:giztoy_dashscope",
        "//rust/cli:giztoy_cli",
    ] + all_crate_deps(normal = True),
)
